        <briefdescription>
            <para>Hyperbole structure.</para>
        </briefdescription>
        <detaileddescription>
            <para>Hyperbole types supported:</para>
            <para>
                <itemizedlist>
                    <listitem>
                        <para>
                            <formula id="17">$\frac{x^2}{a^2} - \frac{y^2}{b^2} = 1$</formula>
                        </para>
                    </listitem>
                    <listitem>
                        <para>
                            <formula id="18">$\frac{x^2}{a^2} - \frac{y^2}{b^2} = -1$</formula>
                        </para>
                    </listitem>
                    <listitem>
                        <para>
                            <formula id="13">$xy = k$</formula>
                        </para>
                    </listitem>
                    <listitem>
                        <para>
                            <formula id="15">$y = \frac{ax + b}{cx + d}$</formula>
                        </para>
                    </listitem>
                </itemizedlist>
            </para>
        </detaileddescription>
